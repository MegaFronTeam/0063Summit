mixin footer()
	+b.FOOTER.footer.section
		.container
			+e.row.row
				.col
					+e.logo-wrap(href='/'): +img(src="img/svg/logo.svg", alt="")
					+e.text
						.h6.ttu 3-5 декабря 2024г.
						p Лекторий Музея «Гараж»
				.col-md-3
						+mnu()
				.col-auto
					.social-icons
						a(href="#"): +icon('tg')
						a(href="#"): +icon('vk')
				.col-12
					+e.copyr
						p Все права защищены
						p Сайт разработан #[a(href="#") Магвай]
		+b.running-line
			+e.item регистрация
			+e.item
				+img(src="img/svg/run-line-5.svg", alt="")
				+img(src="img/svg/run-line-6.svg", alt="")
			+e.item регистрация
			+e.item
				+img(src="img/svg/run-line-4.svg", alt="")
			+e.item регистрация
			+e.item
				+img(src="img/svg/run-line-5.svg", alt="")
				+img(src="img/svg/run-line-6.svg", alt="")
			+e.item регистрация
			+e.item
				+img(src="img/svg/run-line-4.svg", alt="")
			+e.item регистрация
			+e.item
				+img(src="img/svg/run-line-5.svg", alt="")
				+img(src="img/svg/run-line-6.svg", alt="")
			+e.item регистрация
			+e.item
				+img(src="img/svg/run-line-4.svg", alt="")
			+e.item регистрация